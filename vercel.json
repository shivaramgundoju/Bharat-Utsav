{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_OPENROUTER_API_KEY": "@openrouter_api_key"
  },
  "build": {
    "env": {
      "VITE_OPENROUTER_API_KEY": "@openrouter_api_key"
    }
  }
}
